RewriteEngine On 
RewriteRule ^register/initialize?$ RegistrationInitializeEndpoint.php [NC,L]
RewriteRule ^register/submit?$ RegistrationSubmitEndpoint.php [NC,L]
RewriteRule ^login?$ LoginEndpoint.php [NC,L]
RewriteRule ^authenticated/profile?$ ProfileEndpoint.php [NC,L]
RewriteRule ^authenticated/profile/security/settings?$ SecuritySettingsEndpoint.php [NC,L]
RewriteRule ^authenticated/profile/notification/settings?$ endpoint/NotificationSettingsEndpoint.php [NC,L]
RewriteRule ^authenticated/payment/receive?$ ReceivePaymentEndpoint.php [NC,L]
RewriteRule ^authenticated/wallet/initialize?$ InitializeWalletEndpoint.php [NC,L]
RewriteRule ^authenticated/wallet/depositoptions?$ endpoint/WalletDepositOptionsEndpoint.php [NC,L]
RewriteRule ^authenticated/wallet/withdrawoptions?$ endpoint/WalletWithdrawOptionsEndpoint.php [NC,L]
RewriteRule ^authenticated/wallet/paymentoptions?$ endpoint/WalletPaymentOptionsEndpoint.php [NC,L]